{
  "name": "xendit-node",
  "version": "5.0.0",
  "description": "OpenAPI client for xendit-node",
  "author": "OpenAPI-Generator",
  "repository": {
    "type": "git",
    "url": "https://github.com/xendit/xendit-node.git"
  },
  "files": [
    "invoice/**",
    "balance_and_transaction/**",
    "refund/**",
    "payout/**",
    "payment_request/**",
    "payment_method/**",
    "customer/**",
    "docs/**",
    "images/**",
    "README.md",
    "package.json",
    "index.js",
    "index.d.ts",
    "runtime.js",
    "runtime.d.ts"
  ],
  "main": "./index.js",
  "typings": "./index.d.ts",
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "custom-publish": "cp README.md dist/ && cp -R docs dist/ && cp -R images dist/ && cp package.json dist && cd dist && npm publish",
    "test": "DEVELOPMENT_API_KEY=$DEVELOPMENT_API_KEY BUSINESS_ID=$BUSINESS_ID jest",
    "lint": "npx eslint ."
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testPathIgnorePatterns": ["dist/"]
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.6",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "dotenv": "^16.3.1",
    "eslint": "^8.53.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^4.0"
  }
}
